<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bo.gaceta.rcb.modelo">
  <class name="tbInMatriculas" table="tbinmatriculas">
    <id column="idMatricula" name="idMatricula" type="string">
      <generator class="assigned"/>
    </id>
    <property column="razonSocial" name="razonSocial" type="string"/>
    <property column="tipoSocietario" name="tipoSocietario" type="int"/>
    <property column="correoElectronico" name="correoElectronico" type="string"/>
  </class>
</hibernate-mapping>
