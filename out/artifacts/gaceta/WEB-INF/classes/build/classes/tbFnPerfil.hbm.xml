<?xml version="1.0" encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bo.gaceta.rcb.modelo">
  <class name="tbFnPerfil" table="tbfnperfil" >
    <id name="idFuncion" column="idFuncion" type="int">
      <generator class="assigned"/>
    </id>
    <property name="funcion" column="funcion" type="string"/>
    <property name="pageFuncion" column="pageFuncion" type="string"/>
    <property name="icon" column="icon" type="string"/>
    <property name="ctrMenu" column="ctrMenu" type="int"/>
  </class>
</hibernate-mapping>